<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Skribbl.io Mediathek</title>
  <link rel="icon" href="/public/favicon.png">
  <script defer src="/pages/script.js"></script>
  <style>
    .image:hover {
      opacity: 0.95;
    }
  </style>
</head>
<body style="margin-top: 17.5px; background-image: url(/public/background.png); background-repeat: repeat; background-size: 350px;">
  <% images.forEach((image, index) => {
    %>
      <img
        class="image"
        src="<%= image %>"
        style="<%= 'width: 22.5vw; margin-top: 12.5px; margin-left: ' + ((!(index % 4)) * 2.5) + 'vw; margin-right: 12.5px; border-radius: 10px;' %>"
      >
    <%
  }); %>
</body>
</html>